   "source": [
    "H = np.matrix([[2, 0],\n",
    "              [0, 2]])\n",
    "g = np.matrix([[-6],\n",
    "               [-6]])\n",
    "x = np.zeros((2, 1))\n",
    "alpha = 0.1\n",
    "\n",
    "for count in range(100):\n",
    "    df = H * x + g\n",
    "    x = x - alpha * df\n",
    "    \n",
    "    print(f\"# {count}\\n{x}\")\n"
   ]
